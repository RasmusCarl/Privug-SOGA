data ans1 = [1, 1, 1];
data ans2 = [0, 1];

bias1 = beta([1,2], 2);
bias2 = beta([1,3], 2);

for i in range(3) {
    if beta([0.1,1],2) - bias1 < 0 {
        ansb1 = 1;
    } else {
        ansb1 = 0;
    } end if;
    
    observe(ansb1 == ans1[i]);
    
}end for;

for i in range(2) { 

    if beta([0.1,1],2) - bias2 < 0 {
        ansb2 = 1;
    } else {
        ansb2 = 0;
    } end if;

    observe(ansb2 == ans2[i]);
    
} end for;
