array[10] income;
array[10] mask;

for i in range(10) {
    income[i] = uniform([10000, 50000], 2);
} end for;

for i in range(10) {
    if income[i] < 20000 {
        mask[i] = 0;
    } else {
        if income[i] < 30000 {
            mask[i] = 1;
        } else {
            if income[i] < 40000 {
                mask[i] = 2;
            } else {
                mask[i] = 3;
            } end if;
        } end if;
    } end if;
    prune(3000);
} end for;

